    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ext=ts,tsx --fix",
      "stylelint --config=.stylelintrc --fix",
      "prettier --parser typescript --write"
    ],
    "**/*.{css,scss}": [
      "stylelint --config=.stylelintrc --syntax scss --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write",
      "prettier --write"
    ],
    ".*rc": [
      "prettier --parser json --write"
    ]
